<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="warn">
            <!--
            Since we log some expected exceptions during executableLog we disable appender to keep executableLog clean.
             You can temporary uncomment it to perform debugging.
            -->
            <!--<AppenderRef ref="Console"/>-->
        </Root>
    </Loggers>
</Configuration>